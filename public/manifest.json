{
  "manifest_version": 3,
  "name": "InsightGo",
  "version": "1.0",
  "description": "用户数据分析系统.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webRequest",
    "sidePanel",
    "windows",
    "tabs"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        //"<all_urls>",
        "https://developer.chrome.com/docs/extensions*",
        "http://127.0.0.1:3000/*",
        "http://127.0.0.1:8080/*",
        "http://localhost:8000/*",
        "https://console.tensorsecurity.cn/*"
      ],
      "js": ["scripts/content.js"]
    }
  ],
  "host_permissions": [
    "https://developer.chrome.com/docs/extensions*",
    "http://127.0.0.1:3000/*",
    "http://*/*",
    "https://*/*",
    "http://127.0.0.1:8080/*"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html",
    "default_panel": "index.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
